{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby Develop (Debug)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop", "--host", "0.0.0.0"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "GATSBY_TELEMETRY_DISABLED": "1"
      }
    },
    {
      "name": "Gatsby Build (Debug)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["build"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "GATSBY_TELEMETRY_DISABLED": "1"
      }
    }
  ]
}
